<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name> Data Dashboard </display-name>
	<context-param>
		<param-name>dbUrlWindows</param-name>
		<param-value>jdbc:postgresql://LOADGENP1dO0015.vdi.ad.XYZ.com:5432/postgres</param-value>
	</context-param>
	<context-param>
		<param-name>dbUrlUnix</param-name>
		<param-value>jdbc:postgresql://LOADGENP1dO0015.vdi.ad.XYZ.com:5432/postgres</param-value>
	</context-param>
	<context-param>
		<param-name>dbclass</param-name>
		<param-value>org.postgresql.Driver</param-value>
	</context-param>
	<context-param>
        <param-name>logdir</param-name>
        <param-value>C:\DMSV\DataDashboardTempFiles</param-value>
    </context-param>
	
	<listener>
		<listener-class>akhil.initialize.ContextListener</listener-class>
	</listener>
	
	<servlet>
		<servlet-name>sqlregister</servlet-name>
		<display-name>sqlregister</display-name>
		<description></description>
		<servlet-class>akhil.servlets.SQLRegister</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>apicallregister</servlet-name>
		<display-name>apicallregister</display-name>
		<description></description>
		<servlet-class>akhil.servlets.APICallRegister</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>kafkacallregister</servlet-name>
		<display-name>kafkacallregister</display-name>
		<description></description>
		<servlet-class>akhil.servlets.KafkaCallRegister</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>sqlrunner</servlet-name>
		<display-name>sqlrunner</display-name>
		<description></description>
		<servlet-class>akhil.servlets.SQLRunner</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>templatestore</servlet-name>
		<display-name>templatestore</display-name>
		<description></description>
		<servlet-class>akhil.servlets.TemplateStore</servlet-class>
	</servlet>
	<servlet>
        <servlet-name>oauthcreds</servlet-name>
        <display-name>oauthcreds</display-name>
        <description></description>
        <servlet-class>akhil.servlets.OauthSetter</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>datagenerator</servlet-name>
        <display-name>datagenerator</display-name>
        <description></description>
        <servlet-class>akhil.servlets.DataGenerator</servlet-class>
    </servlet>
	
	<servlet-mapping>
    	<servlet-name>datagenerator</servlet-name>
    	<url-pattern>/datagenerator</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
    	<servlet-name>oauthcreds</servlet-name>
    	<url-pattern>/oauthcreds</url-pattern>
    </servlet-mapping>
	<servlet-mapping>
		<servlet-name>sqlregister</servlet-name>
		<url-pattern>/sqlregister</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>apicallregister</servlet-name>
		<url-pattern>/apicallregister</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>kafkacallregister</servlet-name>
		<url-pattern>/kafkacallregister</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>sqlrunner</servlet-name>
		<url-pattern>/sqlrunner</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>templatestore</servlet-name>
		<url-pattern>/templatestore</url-pattern>
	</servlet-mapping>
	
</web-app>
